.degradado{ /* Degradado azul*/
	background: rgb(109,179,242); /* Old browsers */
	background: -moz-linear-gradient(top,  rgba(109,179,242,1) 0%, rgba(84,163,238,1) 50%, rgba(54,144,240,1) 77%, rgba(30,105,222,1) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(109,179,242,1)), color-stop(50%,rgba(84,163,238,1)), color-stop(77%,rgba(54,144,240,1)), color-stop(100%,rgba(30,105,222,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  rgba(109,179,242,1) 0%,rgba(84,163,238,1) 50%,rgba(54,144,240,1) 77%,rgba(30,105,222,1) 100%); /* Chrome10+,Safari5.1+ */
	
	background: -o-linear-gradient(top,  rgba(109,179,242,1) 0%,rgba(84,163,238,1) 50%,rgba(54,144,240,1) 77%,rgba(30,105,222,1) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  rgba(109,179,242,1) 0%,rgba(84,163,238,1) 50%,rgba(54,144,240,1) 77%,rgba(30,105,222,1) 100%); /* IE10+ */
	background: linear-gradient(to bottom,  rgba(109,179,242,1) 0%,rgba(84,163,238,1) 50%,rgba(54,144,240,1) 77%,rgba(30,105,222,1) 100%); /* W3C */


	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#6db3f2', endColorstr='#1e69de',GradientType=0 );  /*IE6-9 */
	/*filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ccc', endColorstr='#333',GradientType=0 ); /* IE6-9 */
	margin:auto;
	text-align: left;
}

.degradado1{/* Degradado gris a negro*/
background: rgb(125,126,125); /* Old browsers */
background: -moz-linear-gradient(top,  rgba(125,126,125,1) 20%, rgba(14,14,14,1) 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(20%,rgba(125,126,125,1)), color-stop(100%,rgba(14,14,14,1))); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  rgba(125,126,125,1) 20%,rgba(14,14,14,1) 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  rgba(125,126,125,1) 20%,rgba(14,14,14,1) 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  rgba(125,126,125,1) 20%,rgba(14,14,14,1) 100%); /* IE10+ */
background: linear-gradient(to bottom,  rgba(125,126,125,1) 20%,rgba(14,14,14,1) 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#7d7e7d', endColorstr='#0e0e0e',GradientType=0 ); /* IE6-9 */
}





#barralateral{
	background-color:#D6D6D6;
	width:135px;
	border: 1px solid #333;
	padding: 3px;
	text-align: center;
	margin-top: 3px;
	height: 587px;
	box-shadow: 3px 3px 3px #333;
	float: left;
}
#firma {
	position: fixed;
	bottom:0px;
	width: 100%;
	font-size: small;
}


#contenido{
	height: 600px;
	text-decoration: none;
	margin-top: 3px;
	width: 100%;
	padding: 0px;
	float: right;
	left: 194px;
	/*top: -55px;*/
}

.rounded{ /* Estilo para redondear borders*/
	border-radius:20px;
	-moz-border-radius:7px; /* //Firefox*/
	-webkit-border-radius:7px; /*//Safari y navegadores que usen WebKit*/
}

pre {
	margin: 0;
	background: #222;
	font-size: 18px;
	color: #999;
	text-shadow: 0px 2px 3px #171717;
	-webkit-box-shadow: 0px 2px 3px #555;
	-moz-box-shadow: 0px 2px 3px #555;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	text-align: center;
	border-radius:10px 10px 0px 0px;
}

