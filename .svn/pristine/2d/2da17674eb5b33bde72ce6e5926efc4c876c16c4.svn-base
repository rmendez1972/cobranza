import java.math.BigDecimal;
import java.util.Date;

public class Iniciaedocta
{
    private Integer id_candidato;
    private String poliza;
    private Date fecha_poliza;
       
    
    public Iniciaedocta(){}
    //constructor que permite crear un objeto
    //Mensaje a partir de los datos del mismo
    public Iniciaedocta(Integer id_candidato,String poliza,Date fecha_poliza)
    {
        this.id_candidato=id_candidato;
        this.poliza=poliza;
        this.fecha_poliza=fecha_poliza;
                
    }
    
    
    
    
    public Integer getId_candidato() {
        return id_candidato;
    }

    public void setId_candidato(Integer id_candidato) {
        this.id_candidato = id_candidato;
    }

    

    public String getPoliza() {
        return poliza;
    }

    public void setPoliza(String poliza) {
        this.poliza = poliza;
    }

    
    public Date getFecha_poliza() {
        return fecha_poliza;
    }

    public void setFecha_poliza(Date fecha_poliza) {
        this.fecha_poliza = fecha_poliza;
    }
}
