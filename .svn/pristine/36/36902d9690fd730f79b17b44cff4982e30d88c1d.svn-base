package javabeans;

import java.math.BigDecimal;
import java.util.Date;

/**
 *
 * @author Rafael Mendez
 */
public class User {

    private String usuario;
    private String password;
    private Integer id_usuario;
    private Integer id_nivel;
    private Integer id_del;
    private String serie;

    public User() {
    }
    //constructor que permite crear un objeto
    //Mensaje a partir de los datos del mismo

    public User(String usuario, String password) {
        this.usuario = usuario;
        this.password = password;


    }

    public User(Integer id_usuario, String usuario, String password, Integer id_nivel, Integer id_del, String serie) {
        this.usuario = usuario;
        this.password = password;
        this.id_usuario = id_usuario;
        this.id_nivel = id_nivel;
        this.id_del = id_del;
        this.serie = serie;


    }

    public String getUsuario() {
        return usuario;
    }

    public void setUsuario(String usuario) {
        this.usuario = usuario;
    }

    public String getPassword() {
        return password;
    }

    public void setPassword(String password) {
        this.password = password;
    }

    public Integer getId_usuario() {
        return id_usuario;
    }

    public void setId_usuario(Integer id_usuario) {
        this.id_usuario = id_usuario;
    }

    public Integer getId_nivel() {
        return id_nivel;
    }

    public void setId_nivel(Integer id_nivel) {
        this.id_nivel = id_nivel;
    }

    public Integer getId_del() {
        return id_del;
    }

    public void setId_del(Integer id_del) {
        this.id_del = id_del;
    }

    public String getSerie() {
        return serie;
    }

    public void setSerie(String serie) {
        this.serie = serie;
    }
}
