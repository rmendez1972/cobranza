package javabeans;



/**
 *
 * @author Rafael Mendez
 */
public class Colonias 
{
   
    
    private Integer idColonia;
    private Integer idDelegacion;
    private String descripcion;
    private String delegacion;
    
    public Colonias(){}
    //constructor que permite crear un objeto
    //Mensaje a partir de los datos del mismo
    public Colonias( Integer idColonia,Integer idDelegacion, String descripcion)
    {
        this.idColonia=idColonia;
        this.idDelegacion=idDelegacion;
        this.descripcion=descripcion;
        
        
    }
    
    
    
    public Colonias( Integer idColonia,String Delegacion, String descripcion)
    {
        this.idColonia=idColonia;
        this.delegacion=Delegacion;
        this.descripcion=descripcion;
        
        
    }
    
    public Colonias( Integer idColonia, Integer idDelegacion,String Delegacion, String descripcion)
    {
        this.idColonia=idColonia;
        this.idDelegacion=idDelegacion;
        this.delegacion=Delegacion;
        this.descripcion=descripcion;
        
        
    }
    
    public Integer getidColonia() {
        return idColonia;
    }

    public void setidColonia(Integer idColonia) {
        this.idColonia = idColonia;
    }
    
    
    public Integer getidDelegacion() {
        return idDelegacion;
    }

    public void setidDelegacion(Integer idDelegacion) {
        this.idDelegacion = idDelegacion;
    }
    
    public String getDescripcion() {
        return descripcion;
    }

    public void setDescripcion(String descripcion) {
        this.descripcion = descripcion;
    }
    
    public String getDelegacion() {
        return delegacion;
    }

    public void setDelegacion(String delegacion) {
        this.delegacion = delegacion;
    }
       
}
